<launch>
  <node pkg="kinfu" type="kinfu" name="kinect_kinfu1" output="screen" ><!--launch-prefix="gdb -ex print &#45;&#45;args"-->
    <param name="prefix_topic" value="" />
    <param name="camera_info_topic" value="/gazebo_depth_cam/depth/camera_info" />
    <param name="depth_image_topic" value="/gazebo_depth_cam/depth/image_raw" />
    <param name="image_topic" value="/gazebo_depth_cam/rgb/image_raw" />
    <param name="first_frame_reference_name" value="world" />
    <param name="current_frame_reference_name" value="ur5_base_link" />

    <param name="autostart" value="true" />
    <param name="response_topic" value="/kinfu_response_topic" />
    <param name="request_topic" value="/kinfu_request_topic" />
    <param name="extract_known_points" value="false" />

    <param name="depth_height" type="int" value="480" />
    <param name="depth_width" type="int" value="640" />
  </node>

</launch>
